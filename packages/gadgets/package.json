{
  "name": "biz-gadgets",
  "version": "1.0.1",
  "description": "业务开发日常小工具库",
  "keywords": [
    "gadgets",
    "utility functions",
    "helper functions",
    "mini lodash"
  ],
  "files": [
    "dist/**/*",
    "package.json"
  ],
  "main": "/dist/umd/gadgets.umd.cjs",
  "module": "/dist/es/index.mjs",
  "types": "/dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/umd/gadgets.umd.cjs"
    },
    "./hooks": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/hooks/index.mjs"
    },
    "./es/*.mjs": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/es/*.mjs"
    }
  },
  "author": "wenzhen",
  "license": "MIT"
}
